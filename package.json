{
  "name": "website",
  "version": "1.0.0",
  "description": "A website for the Broadsage",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "ultracite format",
    "check-types": "turbo run check-types",
    "test": "turbo run test",
    "analyze": "turbo run analyze",
    "translate": "turbo run translate",
    "boundaries": "turbo run boundaries",
    "generate": "turbo gen init",
    "bump-deps": "npx npm-check-updates --deep -u -x react-day-picker",
    "bump-ui": "npx shadcn@latest add --all --overwrite -c packages/design-system",
    "clean": "git clean -xdf node_modules"
  },
  "devDependencies": {
    "@auto-it/first-time-contributor": "^11.3.0",
    "@biomejs/biome": "2.1.3",
    "@repo/typescript-config": "workspace:*",
    "@turbo/gen": "^2.5.5",
    "@types/node": "^24.2.0",
    "require-in-the-middle": "^7.5.2",
    "import-in-the-middle": "^1.14.2",
    "tsup": "^8.5.0",
    "ultracite": "^5.1.2",
    "vitest": "^3.2.4",
    "turbo": "^2.5.5",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.14.0",
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "commander": "^14.0.0"
  }
}
